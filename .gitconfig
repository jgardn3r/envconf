[core]
  editor = vim
  autocrlf = false
  longpaths = true
  quotepath = false
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  ls = ls-files
  cp = cherry-pick
[pull]
  rebase = true
[push]
  default = simple
[branch]
  autosetuprebase = always